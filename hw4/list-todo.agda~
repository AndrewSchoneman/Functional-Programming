take : âˆ€{A : Set} â†’ â„• â†’ ğ•ƒ A â†’ ğ•ƒ A
take zero x =  []
take (suc n) [] = []
take (suc n) ( x :: x') = x :: take  n x' 

intersperse : âˆ€{A : Set} â†’ A â†’ ğ•ƒ A â†’ ğ•ƒ A
intersperse x []  = []
intersperse x ( xs :: [] ) = [ x  ]
intersperse x ( x' :: xs :: xss ) = x' :: x :: xs :: intersperse x ( xss)

